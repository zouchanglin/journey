<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="edu.xpu.journey.entity.mapper.LinkInfoMapper">
    <select id="selectOneById" resultType="edu.xpu.journey.entity.LinkInfo" parameterType="Integer">
        select id, address, description, name from link_info where id = #{id, jdbcType=INTEGER}
    </select>
</mapper>